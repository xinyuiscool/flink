<?xml version="1.0" encoding="UTF-8"?>
<Configuration strict="true" level="trace">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss,SSS} %-5p %-60c %x - %m%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="warn">
      <AppenderRef ref="Console"/>
    </Root>
	<logger name="com.amazonaws" level="DEBUG"/>
	<logger name="com.amazonaws.request" level="DEBUG"/>
	<Logger name="software.amazon.awssdk" level="DEBUG" />
	<Logger name="software.amazon.awssdk.request" level="DEBUG" />
	<!-- Logger name="org.apache.http.wire" level="DEBUG" /-->
  </Loggers>
</Configuration>
